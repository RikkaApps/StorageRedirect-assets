{
    "package": "com.arter97.arktube",
    "verified": false,
    "authors": [
        "nickyc4"
    ],
    "recommendation": "@recommended",
    "behaviors": [
        "@abuse",
        "<p>",
        {
            "en": "The link is to specify the download directory, a feature in donation version. <br>If you are donated user, it is unnecessary to enable this whole rule. ",
            "zh": "下方的链接是用于实现高级版中的自定义下载目录功能。<br>如果您已经购买，则不需要应用重定向或链接。"
        }
    ],
    "export_folders": {
        "data": {
            "downloaded_videos_0": {
                "title": "@downloaded_videos",
                "source": "arkTube",
                "target": "Movies/arkTube",
                "mask": "^(?!.*?\\.temp\\.).*\\.mp4$",
                "call_media_scan": true,
                "allow_child": false
            }
        }
    }
}